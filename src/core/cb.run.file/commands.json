{
    "php": "php %s",
    "py": "python -B %s",
    "js": "node %s",
    "rb": "ruby %s",

    "go": "go run %s",

    "c": "gcc %s -o /tmp/codebox_c_build && /tmp/codebox_c_build && rm /tmp/codebox_c_build",
    "cpp": "g++ %s -o /tmp/codebox_cpp_build && /tmp/codebox_cpp_build && rm /tmp/codebox_cpp_build"
}